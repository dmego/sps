  margin-top: 57rpx;
  padding-bottom: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.theend_text {
  font-size: 24rpx;
  color: rgb(191, 191, 191);
}

/*按钮*/

.culb-manage {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
}

.button {
  width: 100%;
  border-radius: 0;
  padding: 4rpx;
  color: rgb(255, 255, 255);
  background-color: rgb(61, 209, 164);
}

.noimg_view {
  display: flex;
  align-items: center;
  flex-direction: column;
  padding-bottom: 80rpx;
}

.noimg {
  /*width: 141rpx;*/
  height: 115rpx;
  margin-top: 70rpx;
}

.noimg_view text {
  margin-top: 29rpx;
  color: rgb(142, 142, 142);
  font-size: 28rpx;
}

.clubHome-qrcode-tap {
  padding: 20rpx;
  position: absolute;
  right: 15rpx;
  bottom: 18rpx;
}

.clubHome-qrcode {
  width: 66rpx;
  height: 66rpx;
}

.clubHome-option-right {
  padding-top: 50rpx;
  padding-left: 25rpx;
  width: 18rpx;
  height: 31rpx;
  opacity: 0.8;
}

.clubHome-right {
  display: flex;
  flex-direction: column;
}

.item-show-removeBroder {
  border-bottom: '';
}

.master-project-overflow {
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.txt-overflow-one {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.txt-overflow-two {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  /*行数*/
  -webkit-box-orient: vertical;
}

.activity-title {
  height: 74rpx;
}

.apply-time {
  margin: 14rpx 0 13rpx;
}

/*改变活动状态*/

.change-select {
  position: relative;
  padding-left: 15px;
  padding-right: 30px;
  height: 2.58823529em;
  min-height: 2.58823529em;
  line-height: 2.58823529em;
  border-left: 1rpx solid #d9d9d9;
}